{
  "name": "cover-this",
  "version": "1.0.0",
  "description": "The main web application for Family Law Tool Kits",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && npm run watch",
    "build": "npm run build-server && npm run build-client && npm run tslint",
    "serve": "nodemon server/dist/index.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript-Server,Typescript-Client,Node\" -c \"cyan.bold,cyan.bold,green.bold\" \"npm run watch-server\" \"npm run watch-client\" \"npm run serve\"",
    "build-server": "tsc -p ./server",
    "watch-server": "tsc -w -p ./server",
    "build-client": "webpack --display-error-details",
    "watch-client": "webpack -w --display-error-details",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect server/dist/index.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript-Server,Typescript-Client,Node\" -c \"cyan.bold,cyan.bold,green.bold\" \"npm run watch-ts-server\" \"npm run watch-ts-client\" \"npm run serve-debug\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Technosasquach/FLTK_WebApp.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Technosasquach/FLTK_WebApp/issues"
  },
  "homepage": "https://github.com/Technosasquach/FLTK_WebApp#readme",
  "devDependencies": {
    "@types/node": "^10.3.3",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.10.0",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.0.2",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslint": "^5.9.1",
    "typescript": "^2.8.1"
  },
  "dependencies": {
    "@types/axios": "^0.14.0",
    "@types/bcrypt-nodejs": "0.0.30",
    "@types/body-parser": "^1.16.8",
    "@types/compression": "0.0.36",
    "@types/cookie-parser": "^1.4.1",
    "@types/errorhandler": "0.0.32",
    "@types/express": "^4.11.1",
    "@types/lusca": "^1.5.0",
    "@types/morgan": "^1.7.35",
    "@types/react": "^16.3.12",
